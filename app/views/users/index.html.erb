<div class="page-header">
  <div class="columns">
    <div class="first-column">
      <%= content_tag :h1, 'Users' %>
    </div>

    <div class="second-column">
      <%= link_to 'Add User', new_user_path,
                  class: 'button button-success' %>
    </div>
  </div>
</div>


<table class="users">
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th>Team</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <%= render @users %>
  </tbody>
</table>
